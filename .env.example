# Supabase (get from https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api)
SUPABASE_URL=https://your-project-id.supabase.co

# New Supabase API keys (2024+)
SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxx  # Safe for browser
SUPABASE_SECRET_KEY=sb_secret_xxx            # Keep secret! For backend only

# Legacy keys (still supported)
# SUPABASE_ANON_KEY=eyJ...
# SUPABASE_SERVICE_KEY=eyJ...

# Regulations.gov API (get from https://api.data.gov/signup/)
REGULATIONS_API_KEY=your-regulations-api-key

# Anthropic (get from https://console.anthropic.com/)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# OpenAI for embeddings (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-key-here
